using System;

class Program
{
    static void Main()
    {
        Console.Write("Podaj liczbę N: ");
        int N = int.Parse(Console.ReadLine());

        if (N < 1)
        {
            Console.WriteLine("Liczba N musi być większa niż 0.");
            return;
        }

        for (int i = 1; i <= N; i++)
        {
            if (i % 2 == 0)
            {
                Console.Write(i + " ");
            }
        }
    }
}