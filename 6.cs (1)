using System;

class Program
{
    static void Main()
    {
        Console.Write("Podaj liczbÄ™ N: ");
        int N = int.Parse(Console.ReadLine());

        int sum = 0;
        for (int i = 1; i <= N; i++)
        {
            sum += i;
        }

        Console.WriteLine("Suma liczb od 1 do " + N + " wynosi: " + sum);
    }
}