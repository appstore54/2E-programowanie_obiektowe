using System;

class Program
{
    static void Main()
    {
        int number = 1;
        do
        {
            if (number % 2 == 1 && number % 3 == 1 && number % 4 == 1 && number % 5 == 1 && number % 6 == 1 && number % 7 == 0)
            {
                Console.WriteLine("Najmniejsza liczba to: " + number);
                break;
            }
            number++;
        }
        while (true);
    }
}